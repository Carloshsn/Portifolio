* {
    margin: 10px;
}
body {
    background-color: black;
}
div{
    color: blanchedalmond;
}
.nome {
    font-family: Verdana, Geneva, Tahoma, sans-serif;
    font-size: 20px;
    text-align: center;
    padding: 50px;
}
nav{
    color: blanchedalmond;
    display: flexbox;
    flex: 1;
    float: right;
    width: 30%;
    height: 50%;
}
.ficha {
    font-family: Arial, Helvetica, sans-serif;
    font-size: 15px;
    margin: 10px;
}
.foto img{    
    width: 300px;
    height: 350px;
    float: right;
    position: relative;
    margin-right: 50px;
    padding-bottom: 100px;
}
.objetivo{
    margin: 50px;
}
.habilidades{
    margin: 10px;
}
.extras{
    margin: 10px;
}
